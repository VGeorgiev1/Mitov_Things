<p id="notice"><%= notice %></p>

<h1>Team Member Connections</h1>

<table>
  <thead>
    <tr>
      <th>Member</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @team_member_connections.each do |team_member_connection| %>
      <tr>
        <td><%= team_member_connection.member_id %></td>
        <td><%= team_member_connection.team_id %></td>
        <td><%= link_to 'Show', team_member_connection %></td>
        <td><%= link_to 'Edit', edit_team_member_connection_path(team_member_connection) %></td>
        <td><%= link_to 'Destroy', team_member_connection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team Member Connection', new_team_member_connection_path %>
